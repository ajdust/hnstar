{
  "compilerOptions": {
    "target": "ES2017",
    "strict": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": false,
    "moduleResolution": "Node",
    "jsx": "react",
  }
}
